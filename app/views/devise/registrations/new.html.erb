<!DOCTYPE html>
<html>
<head>
  <title>Cadastro</title>
   <!--Made with love by Mutiullah Samim -->
   
  <!--Bootsrap 4 CDN-->
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    
    <!--Fontawesome CDN-->
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">

  <!--Custom styles-->
  <link rel="stylesheet" type="text/css" href="styles.css">
</head>
<body style="background-image: url(<%= asset_path("http://getwallpapers.com/wallpaper/full/a/5/d/544750.jpg")%>);">
<div class="container">
  <div class="d-flex justify-content-center h-100">
    <div class="card">
      <div class="card-header">
        <%= stylesheet_link_tag "new"%>

        <h3>Sign Up</h3>
        
      </div>
        <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= render "devise/shared/error_messages", resource: resource %>
      <div class="card-body">
        <!-- <form> -->
          <div class="input-group form-group">
            <div class="input-group-prepend">

              <span class="input-group-text">
                <i class="fas fa-user"></i>
              </span>
               
              <!-- <%= f.email_field :email, autofocus: true, autocomplete: "email" %> -->
            </div>
            <!-- <%= f.email_field :email, autofocus: true, autocomplete: "email"%>  -->
            <input type="text" class="form-control" placeholder="Email"  <%= f.email_field :email, autofocus: true, autocomplete: "email"%>> 
            
          </div>
          <div class="input-group form-group">
            <div class="input-group-prepend">
              <br>
            <!--   <% if @minimum_password_length %>
                <em>(<%= @minimum_password_length %> characters minimum)</em>
                <% end %><br> -->
            
              <span class="input-group-text"><i class="fas fa-key"></i></span>
            </div>
            <input type="password" class="form-control" placeholder="new password" <%= f.password_field :password, autocomplete: "new-password" %>>
          </div>

           <div class="input-group form-group">
            <div class="input-group-prepend">
              
              <span class="input-group-text"><i class="fas fa-key"></i></span>
            </div>
            <input type="password" class="form-control" placeholder="password confirmation" <%= f.password_field :password_confirmation, autocomplete: "new-password" %>>
          </div>



          <div class="row align-items-center remember">
            <input type="checkbox">Remember Me
          </div>
          <div class="form-group">
            <%= f.submit "Sign up" %>
            <input type="submit" value="Login" class="btn float-right login_btn"> 
            <%= render "devise/shared/links" %>
          </div>
        <!-- </form> -->
        <% end %>

        <!-- <%= render "devise/shared/links" %> -->
      </div>
      
    </div>
  </div>
</div>
</body>
</html>


